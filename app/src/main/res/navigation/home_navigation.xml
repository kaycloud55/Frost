<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_navigation"
    app:startDestination="@id/onboardingFragment">

    <fragment
        android:id="@+id/onboardingFragment"
        android:name="com.kaycloud.frost.ui.OnboardingFragment"
        android:label="OnboardingFragment">
        <action
            android:id="@+id/action_onboardingFragment_to_gankListFragment"
            app:destination="@id/gank" />
    </fragment>

    <fragment
        android:id="@+id/gank"
        android:name="com.kaycloud.frost.module.image.gank.ui.GankListFragment"
        android:label="GankListFragment" />
    <fragment
        android:id="@+id/wallhaven"
        android:name="com.kaycloud.frost.module.image.wallhaven.ui.WallhavenListFragment"
        android:label="WallhavenListFragment" />
    <fragment
        android:id="@+id/toplist"
        android:name="com.kaycloud.frost.module.toplist.ui.toplist.TopListFragment"
        android:label="TopListFragment" />


</navigation>